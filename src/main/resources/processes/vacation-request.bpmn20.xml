    </sequenceFlow>
    <endEvent id="EndEvent" name="End Event">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-381E38DD-C43B-4D36-8BB1-4C96F4E90A5C]]></modeler:editor-resource-id>
